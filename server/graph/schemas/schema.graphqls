# GraphQL schema example
#
# https://gqlgen.com/getting-started/
scalar Time
scalar Upload

type AuthPayload {
  token: String!
}

type UserCreate {
  email: String!
  password: String!
}

input AdminSignInInput {
  account: String!
  password: String!
}

input NewUser {
  email: String
  password: String
}

type Query {
  adminSignIn(adminSignInInput: AdminSignInInput): AuthPayload!
}

type Mutation {
  createAccount(input: NewUser!): UserCreate!
}
