# 课程时间
type Day{
  id:ID!
  weekday:Int
  start:Int
  end:Int
}

# 单个课程
type Course {
  id: ID!
  teachersIn: [User]!
  teachersOut: [String]!
  name: String!
  location: String
  type: String
  startWeek: Int
  endWeek: Int
  schedule:[weekday]!
  studentNumber: Int
  createdAt: Time!
  updateAt: Time!
}

# 课程表学期
type Term {
  id: ID!
  Name: String
  courses: [Course]!
  createdAt: Time!
  updateAt: Time!
}

