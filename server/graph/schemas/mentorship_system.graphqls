# 导师制
type MentorshipSystem {
  id: ID!
  projectName: String!
  students: [String]!
  grade: String
  guidanceDate: Time
  createdAt: Time!
  updateAt: Time!
}

extend type Query {
  mentorshipSystem(id: ID!): MentorshipSystem!
  mentorshipSystems(teacherid: ID!): [MentorshipSystem]!
}

