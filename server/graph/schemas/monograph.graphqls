# 专著
type Monograph {
  id: ID!
  teachersIn: [UserExport]!
  teachersOut: [String]
  title: String
  publishDate: Time
  publishLevel: String
  rank: String
  createdAt: Time!
  updateAt: Time!
}

extend type Query {
  monograph(id: ID!): Monograph!
  monographs(teacherid: ID!): [Monograph]!
}

